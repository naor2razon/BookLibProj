<AnchorPane id="AnchorPane" prefHeight="210.0" prefWidth="356.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library.assistants.ui.addbook.BookAddController">
   <children>
